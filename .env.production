# Production Environment Variables

# API Base URL
VITE_API_BASE_URL=https://api.yourapp.com/api

# API Key
# VITE_API_KEY=your_prod_api_key

# WebSocket URL (for real-time chat)
VITE_WS_URL=wss://api.yourapp.com/ws

# App Configuration
VITE_APP_TITLE=AI Chat Assistant
VITE_APP_ENV=production

# Chat Configuration
VITE_CHAT_TIMEOUT=30000
VITE_MAX_MESSAGE_LENGTH=5000

# Feature Flags
VITE_ENABLE_SOUND=true
VITE_ENABLE_TYPING_INDICATOR=true
VITE_ENABLE_MESSAGE_DELETION=true
