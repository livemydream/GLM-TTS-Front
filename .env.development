# Development Environment Variables

# API Base URL (使用 Vite 代理，无需完整 URL)
VITE_API_BASE_URL=/api

# API Key (if needed)
# VITE_API_KEY=your_dev_api_key

# WebSocket URL (for real-time chat)
VITE_WS_URL=ws://localhost:3000/ws

# App Configuration
VITE_APP_TITLE=AI Chat Assistant
VITE_APP_ENV=development

# Chat Configuration
VITE_CHAT_TIMEOUT=30000
VITE_MAX_MESSAGE_LENGTH=5000

# Feature Flags
VITE_ENABLE_SOUND=false
VITE_ENABLE_TYPING_INDICATOR=true
VITE_ENABLE_MESSAGE_DELETION=true
